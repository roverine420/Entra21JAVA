package Lista6;
import java.util.Scanner;
public class ex11 {

	public static void main(String[] args) {
		Scanner tc = new Scanner(System.in);
		int num, numero;
		for(int i = 1; i <= 10; i++) {
			System.out.println("Digite um numero para saber a tabuada");
			num = tc.nextInt();
			numero = num*i;
			numero++;
			System.out.println("Resultado: " +num + "X" + i + "="+numero);
		}

	}

}
